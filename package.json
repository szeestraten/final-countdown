{
  "name": "final-countdown",
  "description": "A time management webapp",
  "version": "1.0.0",
  "author": "Sandor Zeestraten",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --content-base public/",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "serve": "npm run build && firebase serve",
    "deploy": "npm run build && firebase deploy",
    "pretest": "npm run lint",
    "test": "echo \"No tests\"",
    "lint": "eslint . --ext .json .md"
  },
  "dependencies": {
    "cleave.js": "^0.7.17",
    "numeral": "^2.0.6",
    "timer-stopwatch": "^0.2.0",
    "vue": "^2.3.3",
    "vue-analytics": "^4.1.2",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.19.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "file-loader": "^0.9.0",
    "firebase-tools": "^3.9.0",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  }
